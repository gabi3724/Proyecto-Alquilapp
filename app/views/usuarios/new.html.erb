<h1 class="text-center mb-3">Registrarse</h1>

<% if @usuario.errors.any? %>
    <div class="mb-3">
        <% @usuario.errors.full_messages.each do |msj| %>
            <div class="alert alert-warning mx-auto mb-0 text-center w-75 pt-2 pb-2" role="alert">
                <%= msj %>
            </div>
        <% end %>
    </div>
<% end %>


<%= render "form", usuario: @usuario, status: :see_other %>